<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from WelcomeServletService WSDL service -->

  <service name="WelcomeServlet" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="WelcomeServletService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="WelcomeServlet"/>
      <parameter name="className" value="DefaultNamespace.WelcomeServletSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="WelcomeServlet"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="doPost" qname="operNS:doPost" xmlns:operNS="http://DefaultNamespace" soapAction="" >
        <parameter qname="pns:request" xmlns:pns="http://DefaultNamespace" type="tns:anyType" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:response" xmlns:pns="http://DefaultNamespace" type="tns:anyType" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="doPost"/>

      <typeMapping
        xmlns:ns="http://DefaultNamespace"
        qname="ns:>doPostResponse"
        type="java:DefaultNamespace.DoPostResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
